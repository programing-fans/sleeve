
page{
    background-color:#F5F5F5;
}

.small-hover{
    opacity:0.9;
    transform: scale(0.95, 0.95);
}

.react-hover{
    position: relative;
    top: 3rpx;
    left: 3rpx;
    box-shadow: 0px 0px 8px rgba(0, 0, 0, .1) inset;
}

.hidden-text{
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}



