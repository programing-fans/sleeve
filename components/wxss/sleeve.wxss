.small-hover{
    opacity:0.9;
    transform: scale(0.95, 0.95);
}

.medium-hover{
    opacity: 0.8;
    transform: scale(0.85, 0.85);
}

.react-hover{
    position: relative;
    top: 3rpx;
    left: 3rpx;
    box-shadow: 0px 0px 8px rgba(0, 0, 0, .1) inset;
}

.sleeve {
    font-family: sleeve !important;
    font-size: 24rpx;
    font-style: normal;
    color: #157658 !important;
}

.cut1{
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}

.cut{
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}
